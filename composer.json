{
    "name": "xpendi/cnab-boleto",
    "description": "Biblioteca com boletos para o laravel",
    "homepage": "https://github.com/carlostelles/laravel-boleto",
    "license": "MIT",
    "require": {
        "php":                ">5.5.0",
        "ext-intl":           "*",
        "ext-mbstring":       "*",
        "laravel/framework":  "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf":      "^1.8",
        "ext-curl":           "*",
        "ext-json":           "*",
        "ext-openssl":        "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Xpendi\\CnabBoleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xpendi\\CnabBoleto\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": true
    }
}